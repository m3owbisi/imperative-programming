// C program to show function
// call and definition
#include <stdio.h>

// Function that takes two parameters 
// a and b as inputs and returns 
// their sum
int sum(int a, int b) 
{ 
return a + b; 
}

int diff(int a, int b) 
{ 
return a - b; 
}

int prod(int a, int b) 
{ 
return a * b; 
}

int div(int a, int b) 
{ 
return a / b; 
}

// Driver code
int main()
{
// Calling sum function and 
// storing its value in add variable
int add = sum(10, 30);
int sub = diff(10, 30);
int mul = prod(10, 30);
int divsion = div(10, 30);

printf("Sum is: %d", add);
printf("Diff is: %d", sub);
printf("Prod is: %d", mul);
printf("Div is: %d", division);

return 0;
}
