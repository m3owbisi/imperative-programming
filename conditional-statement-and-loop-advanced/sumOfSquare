#incIude<stdio.h>
include <conio.h>
void main()
{
int num1, rem;
int sum=0;
clrscr();
pritnf(“Enter the number : “);
scanf(“%d”,&num1);
while(num1>0)
{
rem = num1 % 10;
sum = sum + rem * rem;
num1 = num1 / 10;
}
printf("The sum of square of digits is %d", sum);
clrscr();
return 0;
}
