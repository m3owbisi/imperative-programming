#include<stdio.h>
#include<conio.h>
void main()
{
	double a = 10.14;
	double b = 20.23;
	double c = 30.32;
	double d = 40.41;
	clrscr();
	printf(“The value of a is %lf”,a);
	printf(“The value of b is %lf”,b);
	printf(“The value of c is %lf and The value of d is %lf”,c,d);
	getch();
	return 0;
}
