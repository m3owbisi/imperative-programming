#include<stdio.h>
#include<conio.h>
void main()
{
	//variable declaration
	int a , b, ans;
	clrscr();
	printf(“Enter the value of a: ”);
	scanf(“%d”, &a);
	printf(“Enter the value of b: “);
	scanf(“%d”,&b);
	ans = a*b;
	printf(“Multiplication of %d and %d is %d”,a,b,ans);
	getch();
return 0;
}
