#incIude<stdio.h>
include <conio.h>
void main()
{
int num1, rem;
int reverse=0;
clrscr();
pritnf(“Enter the number : “);
scanf(“%d”,&num1);
original = num1;
while(num1>0)
{
rem = num1 % 10;
reverse = reverse * 10 + rem;
num1 = num1 / 10;
}
if (original == reverse) 
{
        	printf(“The given number is a palindrome number.\n", );
} 
else 
{
printf("The given number is not a palindrome number.\n ");
}
clrscr();
return 0;
}
